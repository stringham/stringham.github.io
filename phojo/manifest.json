{
    "short_name": "Phojo",
    "name": "Phojo",
    "description": "Your personal photo journal.",
    "icons": [
        {
            "src": "/phojo/logo.svg",
            "type": "image/svg+xml",
            "sizes": "any",
            "purpose": "any"
        },
        {
            "src": "/phojo/icons/icon-192x192.png",
            "type": "image/png",
            "sizes": "192x192",
            "purpose": "any"
        },
        {
            "src": "/phojo/icons/icon-512x512.png",
            "type": "image/png",
            "sizes": "512x512",
            "purpose": "any"
        },
        {
            "src": "/phojo/icons/maskable-icon-192x192.png",
            "type": "image/png",
            "sizes": "192x192",
            "purpose": "maskable"
        },
        {
            "src": "/phojo/icons/maskable-icon-512x512.png",
            "type": "image/png",
            "sizes": "512x512",
            "purpose": "maskable"
        }
    ],
    "start_url": "/phojo/",
    "display": "standalone",
    "background_color": "#ffffff",
    "theme_color": "#2094f3",
    "scope": "/phojo/",
    "share_target": {
        "action": "/phojo/share-target",
        "method": "POST",
        "enctype": "multipart/form-data",
        "params": {
            "title": "title",
            "text": "text",
            "url": "url",
            "files": [
                {
                    "name": "shared_files",
                    "accept": ["image/*"]
                }
            ]
        }
    }
}
